on join:
	set {-online::%player%} to true
	while {-online::%player%} is true:
		#send "&7[&a&lPlaytopia &c&lNetwork&7] &aComme vous êtes en ligne depuis 1 heure ou plus, nous vous offrons 10€!" to player
